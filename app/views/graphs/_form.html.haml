= form_for(@graph) do |f|
  - if @graph.errors.any?
    #error_explanation
      %h2
        = pluralize(@graph.errors.count, "error")
        prohibited this graph from being saved:
      %ul
        - @graph.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :query_id
    %br/
    = f.number_field :query_id
  .actions
    = f.submit
