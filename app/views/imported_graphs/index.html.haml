%h1 Listing imported_graphs
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - @imported_graphs.each do |imported_graph|
      %tr
        %td= link_to 'Show', imported_graph
        %td= link_to 'Edit', edit_imported_graph_path(imported_graph)
        %td= link_to 'Destroy', imported_graph, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Imported graph', new_imported_graph_path
