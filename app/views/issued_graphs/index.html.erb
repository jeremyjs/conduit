<h1>Listing issued_graphs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issued_graphs.each do |issued_graph| %>
      <tr>
        <td><%= link_to 'Show', issued_graph %></td>
        <td><%= link_to 'Edit', edit_issued_graph_path(issued_graph) %></td>
        <td><%= link_to 'Destroy', issued_graph, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issued graph', new_issued_graph_path %>
