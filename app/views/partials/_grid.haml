.gridster
  %ul.grid
    - @widgets.each_with_index do |w, i|
      %li.grid-item{"data-col" => 1,
                    "data-row" => 1,
                    "data-sizex" => w[:width],
                    "data-sizey" => w[:height]}
        - if w.class == Graph
          = render partial: 'widgets/widget', object: w
        - elsif w.class == QueryTable
          = render partial: 'widgets/table', object: w
        - else
          = render partial: 'widget/widget', object: w

