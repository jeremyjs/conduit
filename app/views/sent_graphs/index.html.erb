<h1>Listing sent_graphs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sent_graphs.each do |sent_graph| %>
      <tr>
        <td><%= link_to 'Show', sent_graph %></td>
        <td><%= link_to 'Edit', edit_sent_graph_path(sent_graph) %></td>
        <td><%= link_to 'Destroy', sent_graph, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sent graph', new_sent_graph_path %>
