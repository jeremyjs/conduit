<h1>Listing query_tables</h1>

<table>
  <thead>
    <tr>
      <th>Query</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @query_tables.each do |query_table| %>
      <tr>
        <td><%= query_table.query_id %></td>
        <td><%= link_to 'Show', query_table %></td>
        <td><%= link_to 'Edit', edit_query_table_path(query_table) %></td>
        <td><%= link_to 'Destroy', query_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Query table', new_query_table_path %>
